{
    "name": "wp-translation-downloader/disable-autorun",
    "require": {
        "johnpbloch/wordpress": "5.5.*@stable",
        "inpsyde/wp-translation-downloader": "dev-master"
    },
    "extra": {
        "wp-translation-downloader": {
            "languages": [
                "de_DE",
                "en_GB"
            ],
            "directory": "languages",
            "auto-run": false
        }
    }
}